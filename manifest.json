
{
    "name": "American Parlimentary Debate Timer",
    "description": "The American Parlimentary Debate Timer. Keeps track of time for you!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["alarms"],
    "action": {
        "browser_action" : {

        "default_popup": "popup.html",
        "default_title": "American Parlimentary Debate Timer"
        },

        "default_icon": {
            "16": "/images/APDA16.png",
            "32": "/images/APDA32.png",
            "48": "/images/APDA48.png",
            "128": "/images/APDA128.png"
        }
    },

    "icons": {
        "16": "/images/APDA16.png",
        "32": "/images/APDA32.png",
        "48": "/images/APDA48.png",
        "128": "/images/APDA128.png"

    },

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}